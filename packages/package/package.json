{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@{org}/{package}",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "type": "module",
  "types": "./typings/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write ../../artifacts/{package} && eslint --fix ../../artifacts/{package}",
    "lint": "prettier --check ../../artifacts/{package} && eslint ../../artifacts/{package}",
    "test": "vitest"
  },
  "keywords": [
    "need",
    "to",
    "spesify",
    "for",
    "every",
    "package"
  ],
  "homepage": "https://github.com/{org}/{repo}",
  "funding": "https://github.com/{org}/{repo}?sponsor",
  "repository": {
    "type": "git",
    "url": "https://github.com/{org}/{repo}.git",
    "directory": "packages/{package}"
  },
  "bugs": {
    "email": "kendinikertenkelebek@me.com",
    "url": "https://github.com/{org}/{repo}/issues"
  },
  "engines": {
    "node": ">= 22.6.0"
  },
  "license": "LGPL-3.0-or-later",
  "author": "Yankı Küçük <kendinikertenkelebek@me.com>",
  "packageManager": "pnpm@9.7.1"
}
